 @keyword private </xs:documentation>
</xs:annotation>
</xs:attribute>
      <xs:attribute name="selectionDel" type="xs:string" >
<xs:annotation>
<xs:documentation xml:lang="en">
 @keyword private </xs:documentation>
</xs:annotation>

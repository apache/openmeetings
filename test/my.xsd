<xs:documentation xml:lang="en">
  @access private </xs:documentation>
</xs:annotation>
</xs:attribute>
      <xs:attribute name="_msdown" type="xs:string" default="false" >
<xs:annotation>
<xs:documentation xml:lang="en">
  @access private </xs:documentation>
</xs:annotation>
</xs:attribute>

</xs:annotation>
</xs:attribute>
      <xs:attribute name="_mwUpdateDel" type="xs:string" default="null" >
<xs:annotation>
<xs:documentation xml:lang="en">
 @keywords private </xs:documentation>
</xs:annotation>
</xs:attribute>

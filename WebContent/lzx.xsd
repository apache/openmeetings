      <xs:attribute name="onversion" type="xs:string" >
</xs:attribute>
      <xs:attribute name="datastyleindex" type="lzx:number" default="0" >
<xs:annotation>
<xs:documentation xml:lang="en">
 @keyword private </xs:documentation>

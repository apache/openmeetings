<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="private_messages" lazy="false" name="org.openmeetings.app.hibernate.beans.user.PrivateMessages">
    <id column="private_message_id" name="privateMessageId">
      <generator class="increment"/>
    </id>
    <property name="subject" type="string" column="subject"/>
    <property name="message" type="text" column="message"/>
    <property name="inserted" type="java.util.Date" column="inserted"/>
    <many-to-one outer-join="true" not-null="false" column="from_id" lazy="false" cascade="none" name="from" class="org.openmeetings.app.hibernate.beans.user.Users"/>
    <many-to-one outer-join="true" not-null="false" column="to_id" lazy="false" cascade="none" name="to" class="org.openmeetings.app.hibernate.beans.user.Users"/>
    <property name="bookedRoom" type="boolean" column="booked_room"/>
    <many-to-one outer-join="true" not-null="false" column="rooms_id" lazy="false" cascade="none" name="room" class="org.openmeetings.app.hibernate.beans.rooms.Rooms"/>
    <property name="parentMessage" type="long" column="parent_message_id"/>
    <property name="isTrash" type="boolean" column="is_trash"/>
    <property name="privateMessageFolderId" type="long" column="private_message_folder_id"/>
  </class>
</hibernate-mapping>

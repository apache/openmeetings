<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="om_timezone" name="org.openmeetings.app.hibernate.beans.basic.OmTimeZone">
    <id column="om_timezone_id" name="omtimezoneId">
      <generator class="increment"/>
    </id>
    <property name="jname" type="string" column="jname"/>
    <property name="label" type="string" column="label"/>
    <property name="ical" type="string" column="ical"/>
    <property name="inserted" type="java.util.Date" column="inserted"/>
    <property name="orderId" type="int" column="order_id"/>
  </class>
</hibernate-mapping>

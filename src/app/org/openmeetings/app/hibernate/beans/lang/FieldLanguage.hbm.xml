<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="fieldlanguage" name="org.openmeetings.app.hibernate.beans.lang.FieldLanguage">
    <id column="language_id" name="language_id">
      <generator class="increment"/>
    </id>
    <property name="name" type="string" column="name"/>
    <property name="starttime" type="java.util.Date" column="starttime"/>
    <property name="updatetime" type="java.util.Date" column="updatetime"/>
    <property name="deleted" type="string" column="deleted"/>
    <property name="rtl">
      <column name="rtl" sql-type="BOOLEAN"/>
    </property>
  </class>
</hibernate-mapping>

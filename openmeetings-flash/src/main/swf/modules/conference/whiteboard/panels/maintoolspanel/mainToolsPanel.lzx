       leftClassPanel="mainToolsLeftPanelPointer"
       rightClassPanel="mainToolsLeftPanelPointer"
       verticalClassActive="false"
       vWidth="52" vHeight="32">
    
    <verticalMenuIconBarPointer x="2" y="20" />
</class>	

</library>
